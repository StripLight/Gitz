<?xml version="1.0" encoding="UTF-8"?>

<!--
	Clone example manifest file for gitz. See the model for details

	The /temp/my-server must not exist
-->

<manifest>

	<baseFolder>/temp/</baseFolder>
	<description>Manifest to clone /temp/work/server into /temp/my-server</description>

	<defaultServerPath>work/</defaultServerPath>

	<postCommandTasks/>

	<manifestEntries>

		<entry>
			<repository>server</repository>
			<targetName>my-server</targetName>
		</entry>
	</manifestEntries>

</manifest>
